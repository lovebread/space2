<% delivery = video_feed_delivery %>
<% video = delivery.feed_item.originator %>
<div class="feed-list-box">
<div class="feed-list" id='feed_delivery_<%= delivery.id %>'>
	<div class="img left">
		<div class="imgbox01"><%= avatar video.poster %></div>
	</div>
  <div class="jl-list-con jl-cutline">
		<%= profile_link video.poster %> 分享了视频 <%= video_link video, :class => "shine"%>
	<% if delivery.is_deleteable_by? current_user %><%= link_to_remote "", :url => feed_delivery_url(delivery), :method => :delete,:html=>{ :class => "icon-active"} %><% end %> 
    <p class="gray jl-con s_clear" id='video_<%= video.id %>'> <%= video_thumbnail video, :class=>'imgbox01' %></p>
		<p class="gray"><%= ftime video.created_at %></p>
	</div>
</div>
</div>
